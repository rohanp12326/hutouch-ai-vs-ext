{
  "name": "hutouch-ai-coding",
  "displayName": "HuTouch AI Code",
  "description": "Supportive IDE extension for Hutouch AI.",
  "version": "2.10.2",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "publisher": "NiitiLLC",
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rohanp12326/hutouch-ai-vs-ext"
  },
  "icon": "images/icon.png",
  "contributes": {
    "commands": [
      {
        "command": "hutouch.activate",
        "title": "Activate HuTouch"
      },
      {
        "command": "extension.setupStructure",
        "title": "Setup Project Structure"
      }
    ],
    "configuration": {
      "title": "HuTouch AI",
      "properties": {
        "hutouch.apiKey": {
          "type": "string",
          "default": "",
          "description": "HuTouch API key used for sending logs and telemetry from the extension."
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "vscode:uninstall": "node ./scripts/uninstall.js"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "chokidar": "^3.6.0",
    "diff": "^7.0.0",
    "diff-match-patch": "^1.0.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "file-type": "^19.0.0",
    "mime": "^4.0.3",
    "openai": "^4.47.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.89.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  }
}